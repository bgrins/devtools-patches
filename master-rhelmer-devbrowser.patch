+@namespace xul url("http://www.mozilla.org/keymaster/gatekeeper/there.is.only.xul");
+html, body {
+    height: 98%;
+}
+
+body {
+    display: flex;
+    flex-direction: column;
+}
+
+#controls {
+    display: flex;
+    flex-direction: row;
+    padding: 2px;
+    justify-content: space-evenly;
+}
+
+#controls > button {
+    background: none;
+    border: none;
+}
+
+#controls > button:hover {
+    background: #e1e1e1;
+    border-top: 1px solid #d0d0d0;
+}
+
+#controls > #menu {
+    margin-left: auto;
+}
+
+#controls > #addressbar {
+    width: 100%;
+    box-sizing: border-box;
+    background-color: lightgrey;
+}
+
+#browser_container {
+    width: 100%;
+    flex-grow: 1;
+}
+
+.browser_instance {
+    width: 100%;
+    height: 100%;
+}
+
+#menu_popup {
+    visibility: hidden;
+    opacity: 0;
+    position: absolute;
+    transition: all 0.5s ease;
+    margin-top: 1rem;
+    left: 0;
+    display: none;
+}
+
+#menu {
+    visibility: visible;
+    opacity: 1;
+    display: block;
+}
