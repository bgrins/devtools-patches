diff --git a/browser/tools/devbrowser/content/devbrowser.js b/browser/tools/devbrowser/content/devbrowser.js
new file mode 100644
index 0000000000000..7f18d70b80ed1
--- /dev/null
+++ b/browser/tools/devbrowser/content/devbrowser.js
@@ -0,0 +1,156 @@
+if (!document.createXULElement) {
+        throw new Exception("this page must be loaded via the chrome:// protocol");
+}
+
+const { Services } = ChromeUtils.import("resource://gre/modules/Services.jsm");
+const { XPCOMUtils } = ChromeUtils.import("resource://gre/modules/XPCOMUtils.jsm");
+
+XPCOMUtils.defineLazyGetter(this, "gSystemPrincipal", () =>
+    Services.scriptSecurityManager.getSystemPrincipal()
+);
+
+class BrowserUI {
+    constructor() {
+        const browser = document.createXULElement("browser");
+        this.browser = browser;
+
+        browser.setAttribute("id", "browser");
+        browser.setAttribute("type", "content");
+        browser.setAttribute("remote", "true");
+        browser.classList.add("browser_instance");
+
+        document.getElementById("browser_container").appendChild(browser);
+
+        let prevUrl = Services.prefs.getStringPref("devbrowser.currentURL", "https://google.com");
+        // FIXME add a listener to the browser, don't just spin the loop.
+        window.setTimeout(() => {
+                browser.loadURI(prevUrl, { triggeringPrincipal: gSystemPrincipal });
+        }, 0);
+
+        this.controls = document.getElementById("controls");
+        this.back = document.getElementById("back");
+        this.forward = document.getElementById("forward");
+        this.reload = document.getElementById("reload");
+        this.reload_image = document.getElementById("reload-image");
+        this.addressbar = document.getElementById("addressbar");
+
+        this.addListeners();
+    }
+
+    addListeners() {
+        let metaDown = false;
+        document.addEventListener('keydown', (event) => {
+            if (event.key == "Meta") {
+                metaDown = true;
+            }
+
+            if (event.key == "l" && metaDown) {
+                this.addressbar.select();
+            }
+        });
+
+        document.addEventListener('keyup', (event) => {
+            if (event.key == "Meta") {
+                metaDown = false;
+            }
+        });
+
+        controls.addEventListener("click", (event) => {
+            switch(event.target.parentNode.id) {
+                case "back":
+                    this.browser.goBack();
+                    break;
+                case "forward":
+                    this.browser.goForward();
+                    break;
+                case "reload":
+                    if (this.reload_image.src == "chrome://devbrowser/content/assets/reload.svg") {
+                        this.browser.reload();
+                    } else {
+                        this.browser.stop();
+                    }
+                    break;
+            }
+        });
+
+        this.addressbar.onkeypress = (event) => {
+            const returnKey = 13;
+
+            if (event.keyCode == returnKey) {
+                let url;
+                try {
+                    url = new URL(event.target.value);
+                } catch (e) {
+                    if (e.name == "TypeError") {
+                        url = new URL(`https://${event.target.value}`);
+                    }
+                }
+
+                this.browser.setAttribute("type", "content");
+                this.browser.loadURI(url, { triggeringPrincipal: gSystemPrincipal });
+                this.browser.focus();
+            }
+        }
+
+        this.browser.addProgressListener(
+        {
+            QueryInterface: ChromeUtils.generateQI([
+                Ci.nsIWebProgressListener,
+                Ci.nsISupportsWeakReference,
+            ]),
+            onStateChange(webProgress, request, flags, status) {
+                const reload_image = document.getElementById("reload-image");
+                if (flags & Ci.nsIWebProgressListener.STATE_STOP) {
+                    reload_image.src = "assets/reload.svg";
+                } else {
+                    reload_image.src = "assets/stop.svg";
+                }
+            },
+            onLocationChange(webProgress, request, location, flags) {
+                const addressBar = document.getElementById("addressBar");
+                const browser = document.getElementById("browser");
+                const back = document.getElementById("back");
+                const forward = document.getElementById("forward");
+
+                const url = browser.currentURI.spec;
+                // preserve the current URL for BrowserUI restarts, crashes, etc.
+                Services.prefs.setStringPref("devbrowser.currentURL", url)
+
+                addressbar.value = browser.currentURI.spec;
+                back.disabled = !browser.canGoBack;
+                forward.disabled = !browser.canGoForward;
+            },
+            onProgressChange(
+                webProgress,
+                request,
+                curSelfProgress,
+                maxSelfProgress,
+                curTotalProgress,
+                maxTotalProgress
+            ) {},
+            onSecurityChange(webProgress, request, state) {},
+            onContentBlockingEvent(webProgress, request, event) {},
+            onStatusChange: function(
+                webProgress,
+                request,
+                status,
+                message
+            ) {},
+            QueryInterface: ChromeUtils.generateQI([
+                Ci.nsIWebProgressListener,
+            ]),
+        });
+    }
+
+    static restart() {
+        Services.obs.notifyObservers(null, "startupcache-invalidate");
+        window.location.reload();
+    }
+}
+
+try {
+    const browserUI = new BrowserUI();
+} catch(e) {
+    const message = `Browser Error: ${e}`;
+    console.error(message);
+}
