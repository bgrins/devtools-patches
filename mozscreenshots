+++ b/browser/modules/test/browser_screenshots.js
@@ -0,0 +1,34 @@
+/* This Source Code Form is subject to the terms of the Mozilla Public
+ * License, v. 2.0. If a copy of the MPL was not distributed with this
+ * file, You can obtain one at http://mozilla.org/MPL/2.0/. */
+
+const AddonWatcher = Cu.import("resource://gre/modules/AddonWatcher.jsm", {}).AddonWatcher;
+const FileUtils = Cu.import("resource://gre/modules/FileUtils.jsm").FileUtils;
+const TESTROOT = "http://example.com/browser/browser/modules/test/";
+
+function test() {
+  waitForExplicitFinish();
+  requestLongerTimeout(20);
+
+  // Don't let the caret blink since it causes false positives for image diffs
+  Services.prefs.setIntPref("ui.caretBlinkTime", -1);
+
+  info("Installing mozscreenshots extension");
+  AddonManager.getInstallForURL(TESTROOT + "mozscreenshots.xpi", function(aInstall) {
+    aInstall.addListener({
+      onInstallEnded: function(aInstall, aAddon) {
+        AddonManager.getAddonByID(aAddon.id, function(aAddon) {
+          isnot(aAddon, null, "Should have installed the mozscreenshots extension.");
+          AddonWatcher.ignoreAddonPermanently(aAddon.id);
+        });
+      }
+    });
+
+    aInstall.install();
+  }, "application/x-xpinstall");
+}
+
+Services.obs.addObserver(function observer(subject, topic, data) {
+  Services.obs.removeObserver(observer, topic);
+  finish();
+}, "mozscreenshots-finished", false);
