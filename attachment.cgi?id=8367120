+        spectrum.rgb = this._colorToRgba(color);
+        spectrum.updateUI();
+      });
+    });
+
+    dropper.once("destroy", () => {
+      this.tooltip.consumeOutsideClick = true;
+      this.tooltip.preventHiding = false;
+    })
+
+    dropper.open();
+  },
