+"use strict";
+
+{
+
+class MozBrowser extends MozXULElement {
+  connectedCallback() {
+    console.log("MozBrowser");
+  }
+}
+
+customElements.define("browser1", MozBrowser);
+
+
+class MozRemoteBrowser extends MozBrowser {
+  connectedCallback() {
+    console.log("MozRemoteBrowser");
+  }
+}
+
+customElements.define("remote-browser", MozRemoteBrowser, { extends: "browser1" }
+
+}
